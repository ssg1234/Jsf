<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
		http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	
 <managed-bean>
 	<managed-bean-name>ibean</managed-bean-name>
 	<managed-bean-class>com.hexa.bean.IBean</managed-bean-class>
 	<managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
  <navigation-rule>
    	<from-view-id>/IntFrm.jsp</from-view-id>
    	<navigation-case>
    		<from-outcome>calculated</from-outcome>
    		<to-view-id>/IntCal.jsp</to-view-id>
    	</navigation-case>
    	
    	
    	<navigation-case>
    		<from-outcome>cicalculated</from-outcome>
    		<to-view-id>/IntCal.jsp</to-view-id>
    	</navigation-case>
    </navigation-rule>
    
</faces-config>















































